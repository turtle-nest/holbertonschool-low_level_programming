#include "main.h"
/**
 *leet - encodes a string into 1337
 *@str: is a string
 *
 * Description: encodes a string into 1337.
 *
 * Return: 0.
 */

char *leet(char *str) 
{
	char *leet_chars = "43071";
	char *normal_chars = "aeotlAEOTL";
	int i, j;

	for (i = 0; str[i]; i++)
	{
	for (j = 0; normal_chars[j]; j++)
	{
	if (str[i] == normal_chars[j])
	{
	str[i] = leet_chars[j];
	}
	}
	}

	return (str);
}
