#include "main.h"
#include <unistd.h>

char *leet(char *str) {
    char *leet_chars = "43071";
    char *normal_chars = "aeotlAEOTL";
    int i, j;

    for (i = 0; str[i]; i++) {
        for (j = 0; normal_chars[j]; j++) {
            if (str[i] == normal_chars[j]) {
                str[i] = leet_chars[j];
            }
        }
    }

    return str;
}

